<Window x:Class="PowerballWPF.insertwindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PowerballWPF"
        mc:Ignorable="d"
        Title="insertwindow" Height="300" Width="500">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="24"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="24"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="16"/>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="16"/>
        </Grid.RowDefinitions>
        <Image Opacity="1.5" Stretch="UniformToFill" Grid.ColumnSpan="7" Grid.RowSpan="7" Source="/powerball.png"/>
        <Button x:Name="Button01" Grid.Column="1" Grid.Row="2" Content="01" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button01_Click"/>
        <Button x:Name="Button02" Grid.Column="2" Grid.Row="2" Content="02" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button02_Click"/>
        <Button x:Name="Button03" Grid.Column="3" Grid.Row="2" Content="03" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button03_Click"/>
        <Button x:Name="Button04" Grid.Column="4" Grid.Row="2" Content="04" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button04_Click"/>
        <Button x:Name="Button05" Grid.Column="5" Grid.Row="2" Content="05" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button05_Click"/>
        <Button x:Name="Button06" Grid.Column="1" Grid.Row="3" Content="06" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button06_Click"/>
        <Button x:Name="Button07" Grid.Column="2" Grid.Row="3" Content="07" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button07_Click"/>
        <Button x:Name="Button08" Grid.Column="3" Grid.Row="3" Content="08" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button08_Click"/>
        <Button x:Name="Button09" Grid.Column="4" Grid.Row="3" Content="09" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button09_Click"/>
        <Button x:Name="Button10" Grid.Column="5" Grid.Row="3" Content="10" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button10_Click"/>
        <Button x:Name="Button11" Grid.Column="1" Grid.Row="4" Content="11" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button11_Click"/>
        <Button x:Name="Button12" Grid.Column="2" Grid.Row="4" Content="12" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button12_Click"/>
        <Button x:Name="Button13" Grid.Column="3" Grid.Row="4" Content="13" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button13_Click"/>
        <Button x:Name="Button14" Grid.Column="4" Grid.Row="4" Content="14" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button14_Click"/>
        <Button x:Name="Button15" Grid.Column="5" Grid.Row="4" Content="15" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button15_Click"/>
        <Button x:Name="Button16" Grid.Column="1" Grid.Row="5" Content="16" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button16_Click"/>
        <Button x:Name="Button17" Grid.Column="2" Grid.Row="5" Content="17" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button17_Click"/>
        <Button x:Name="Button18" Grid.Column="3" Grid.Row="5" Content="18" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button18_Click"/>
        <Button x:Name="Button19" Grid.Column="4" Grid.Row="5" Content="19" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button19_Click"/>
        <Button x:Name="Button20" Grid.Column="5" Grid.Row="5" Content="20" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="40" Width="70" Margin="2,2,2,2" Background="#FFF7FF00" FontSize="24" FontWeight="Bold" Click="Button20_Click"/>
    </Grid>
</Window>
